@Library("my-shared-library") _
pipeline{

    agent any

    stages{
         
        stage('Git Checkout'){
            steps{
                gitCheckout(
                    branch:"main",
                    url:"https://github.com/asar4rudra/devops_demo_proj.git"
                )
   
                
            }
        }
         stage('Unit Test Maven'){
            steps{
                script{
                    git branch: 'main', url: 'https://github.com/asar4rudra/devops_demo_proj.git'
                }
                
            }
        }     
    }
}